<script>
	$(document).ready(function(){
		$('.left-data').removeClass('col-md-4').addClass('col-md-12');
		$('.left-data-title').addClass('header-tags-close');
		$('.header-tags').css({'display':'none'});
		$('.table-date-click').css({'display':'none'});
		$(".linenums").addClass("prettyprint");
		$("body").attr("onload","prettyPrint();");
	});
	$(document).ready(function(){
		$('.wrapper').on('click','.header-tags-close',function(){
			$('.left-data-title').removeClass('header-tags-close').addClass('header-tags-open');
			//slideDown//slideUp//show//hide//slideUp//slideToggle//fadeIn//fadeOut//fadeTo
			$('.header-tags').fadeIn(1000,function(){
				$('.header-tags').css({'display':'block'})
				$('.table-date-click').css({'display':'inline-table'});
			});
		}).on('click','.header-tags-open',function(){
			$('.left-data-title').removeClass('header-tags-open').addClass('header-tags-close');
			$('.header-tags').hide(1000,function(){
				$('.header-tags').css({'display':'none'});
				$('.table-date-click').css({'display':'none'});
			});
		});
	});
</script>
<style>
	.prettyprint {
		background-color: #141424;
	}
	.kwd, .linenums .tag {
		color: #E562D0;
	}
	.pln {
		color: #0EDBE4;
	}.pun, .opn, .clo {
		color: #2EA016;
	}.str, .atv {
		color: #ED5353;
	}
	ol.linenums li {
		text-shadow:none;color: #05A391;
	}
</style>
<aside id="reader-right" class="col-md-12">
	<% FeelsById.forEach(	function(FeelsById){%>
	<div class="div-reader-title">
		<p class="p-reader-title"><%= FeelsById.kt_feels_titles %></p>
		<a class="a-reader-next a-hover" id="a_reader_next" href="/in/feelsByNext?feels_ids=<%= FeelsById.kt_feels_ids %>&tags_ids=<%= FeelsById.kt_feel_tags_ids %>">Next
		</a>
		<a class="a-reader-prev a-hover" id="a_reader_prev" href="/in/feelsByPrev?feels_ids=<%= FeelsById.kt_feels_ids %>&tags_ids=<%= FeelsById.kt_feel_tags_ids %>">Prev
		</a>
		<a class="a-reader-next a-hover" href="/in/feelsByMenu?tags_ids='<%= FeelsById.kt_feel_tags_ids %>'">Menu
		</a>
	</div>
	<div class="div-reader-text">
		<p class="p-reader-text"><%- FeelsById.kt_feels_contents %></p>
	</div><% });%>
	<input class="tags_open_next" id="tags_open_next" style="height:0px;width:0px;" name="tags_open_next" type="hidden"  value="<%= tags_open_next %>">
	<input class="tags_open_prev" id="tags_open_prev" style="height:0px;width:0px;" name="tags_open_prev" type="hidden"  value="<%= tags_open_prev %>">
</aside>
