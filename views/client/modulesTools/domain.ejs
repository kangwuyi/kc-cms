<article>
	<ul class="nav nav-tabs droptabs" id="droptabs">
		<li class="active always-visible"><a href="#Transaction" data-toggle="tab">Transaction</a></li>
		<li><a href="#Registrar" data-toggle="tab">Registrar</a></li>
		<li><a href="#Suffix" data-toggle="tab">Suffix</a></li>
		<li class="dropdown pull-right">
			<a href="#" id="myTabDrop1" class="dropdown-toggle" data-toggle="dropdown">News <b class="caret"></b></a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="myTabDrop1">
				<li class="always-dropdown"><a href="#news" data-toggle="tab">Instant news</a></li>
			</ul>
		</li>
	</ul>
	<div class="row-fluid ">
		<div class="tab-content span4" id="tab-content">
			<div class="tab-pane active" id="Transaction">
	        				<div class="col-md-12">
						<div class="panel panel-default table-responsive">
		    					<table class="table table-striped table-hover">
	        						<thead>
	        							<tr>
									<th>域名</th>
									<th>语义</th>
									<th>价格</th>
									<th>交易平台</th>
	        							</tr>
	        						</thead>
	        						<tbody>
			        					<tr>
								    	<td><strong>jiuyelv.com</strong></td>
								    	<td>就业率
								    	</td>
								    	<td>待定</td>
								    	<td>易名中国</td>
									</tr>
		    						</tbody>
    							</table>
						</div>
        					</div>
			</div>
			<div class="tab-pane" id="Registrar">
        				<div class="col-md-12">
					<div class="panel panel-default table-responsive">
	    					<table class="table table-striped table-hover">
        						<thead>
        							<tr>
								<th>名称</th>
								<th>地址</th>
								<th>域名后缀列表</th>
        							</tr>
        						</thead>
        						<tbody>
        							<% PostGetDomainsite.forEach(	function(PostGetDomainsite){%>
		        					<tr>
								    	<td><strong><%= PostGetDomainsite.kt_domainsite_name %></strong></td>
								    	<td>
								       	<a href="<%= PostGetDomainsite.kt_domainsite_href %>" target="_blank" rel="nofollow"><%= PostGetDomainsite.kt_domainsite_href %>
								       	</a>
								    	</td>
								    	<td><%= PostGetDomainsite.kt_domainsite_text %></td>
								</tr>
								<% });%>
	    						</tbody>
							</table>
					</div>
				</div>
			</div>
			<div class="tab-pane" id="Suffix">
        				<div class="col-md-12">
					<div class="panel panel-default table-responsive">
    						<table class="table table-striped table-hover">
        						<thead>
        							<tr>
									<th>顶级域名</th>
									<th>类型</th>
									<th>组织</th>
									<th>官方网站</th>
        							</tr>
        						</thead>
        						<tbody>
        							<% PostGetDomainsuffix.forEach(	function(PostGetDomainsuffix){%>
		        					<tr>
								    	<td><strong><a href="<%= PostGetDomainsuffix.kt_domainsuffix_name_href %>"><%= PostGetDomainsuffix.kt_domainsuffix_name %></a></strong></td>
								    	<td><%= PostGetDomainsuffix.kt_domainsuffix_type %></td>
								    	<td><%= PostGetDomainsuffix.kt_domainsuffix_org %></td>
								    	<td>
				        					<a href="<%= PostGetDomainsuffix.kt_domainsuffix_url %>" target="_blank" rel="nofollow"><%= PostGetDomainsuffix.kt_domainsuffix_url %></a>
									</td>
								</tr>
								<% });%>
		    					</tbody>
    						</table>
					</div>
    				</div>
			</div>
			<div class="tab-pane" id="news">待定</div>
		</div>
	</div>
</article>